        <GrxSingleSelection android:title="Ecran Bureau" android:key="fbv_st_action"  android:defaultValue="0"
            grx:optionsArray="@array/mapping_all_options" grx:valuesArray="@array/mapping_all_options_values"
            grx:clickRules="-1#fbv_st_action_uri#,NULL,"/>

        <GrxAccess android:title="Appli, activité ou raccourci simple pression" android:key="fbv_st_action_uri"
            grx:depRule="ENABLE#fbv_st_action#STRING#-1"
            grx:showShortcuts="true" grx:showApplications="true" grx:showActivities="true" grx:arrowColor="?complementary_color1" />

        <GrxButtonPreference android:title="" android:key="rkbutton0"   grx:button_Style="big"
            grx:button_Text="Pressez pour appliquer" grx:button_backgroundColor="?complementary_color3" grx:commonBcExtra="UPDATE" grx:commonBcExtraValue="FBV_ACTIONS" />


