    <PreferenceScreen android:title="B.F Actions simple pression" android:key="pst"  >

        <GrxPreferenceCategory android:title="Action simple pressionp" />

        <GrxButtonPreference android:title="" android:key="rkbutton1"   grx:button_Style="big"
